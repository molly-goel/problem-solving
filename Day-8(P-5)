#include <bits/stdc++.h>

using namespace std;

int main()
{
   int n,i;
   cin>>n; 
   for(i=1;i<=n;i++)
   {
       if(i==1)
       {
           cout<<"1";
       }
       else if(i%2==0)
       {
           cout<<"2";
       }
       else
       {
           if(i%3==0)
           {
               cout<<"3";
           }
            else if(i%5==0)
           {
               cout<<"5";
           }
            else 
           {
               cout<<"7";
           }
       }
   }
    
  
    return 0;
}
